{
  "version": 2,
  "builds": [
    {
      "src": "package.json", 
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/TAG_Logo\\.png",
      "dest": "/TAG_Logo.png",
      "headers": {
        "Content-Type": "image/png"
      }
    },
    {
      "src": "/TAG_mini_logo\\.png", 
      "dest": "/TAG_mini_logo.png",
      "headers": {
        "Content-Type": "image/png"
      }
    },
    {
      "src": "/sitemap\\.xml",
      "dest": "/sitemap.xml"
    },
    {
      "src": "/robots\\.txt", 
      "dest": "/robots.txt"
    },
    {
      "src": "/favicon\\.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(png|jpg|jpeg|gif|ico|svg))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "GENERATE_SOURCEMAP": "false",
    "NODE_ENV": "production", 
    "CI": "false"
  }
}
